import java.util.Scanner;

public class AbsentDigits {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        System.out.print("Enter a number: ");
        String number = sc.nextLine();   // take as String to access each digit

        boolean[] present = new boolean[10];  // index 0 to 9

        // Mark the digits that are present in the number
        for (int i = 0; i < number.length(); i++) {
            char ch = number.charAt(i);
            if (Character.isDigit(ch)) {
                int digit = ch - '0';
                present[digit] = true;
            }
        }

        System.out.println("\nAbsent digits are:");

        // Print digits that are not present
        for (int i = 0; i <= 9; i++) {
            if (!present[i]) {
                System.out.print(i + " ");
            }
        }

        sc.close();
    }
}
