class Square {

    // Method to find square of an integer
    int findSquare(int n) {
        return n * n;
    }

    // Method to find square of a float
    float findSquare(float n) {
        return n * n;
    }

    // Method to find square of a double
    double findSquare(double n) {
        return n * n;
    }
}

public class SquareOverloadingDemo {

    public static void main(String[] args) {

        Square obj = new Square();

        int intNum = 5;
        float floatNum = 4.5f;
        double doubleNum = 6.25;

        // Calling overloaded methods
        System.out.println("Square of Integer " + intNum + " = " + obj.findSquare(intNum));
        System.out.println("Square of Float " + floatNum + " = " + obj.findSquare(floatNum));
        System.out.println("Square of Double " + doubleNum + " = " + obj.findSquare(doubleNum));
    }
}
